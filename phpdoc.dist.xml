<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        configVersion="3"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://docs.phpdoc.org/latest/phpdoc.xsd"
>
    <title>Mspecs WordPress Plugin API Documentation</title>
    <paths>
        <output>docs</output>
        <cache>build/cache</cache>
    </paths>
    <version number="3.0.0">
        <api>
            <source dsn=".">
                <path>public-api.php</path>
            </source>
            <output>api</output>
        </api>
        <guide>
            <source dsn=".">
                <path>guide</path>
            </source>
            <output>guide</output>
        </guide>
    </version>
    <templates>
        <template name="clean"/>
    </templates>
</phpdocumentor>